model QuestionForm {
    id              Int           @id @default(autoincrement())
    uuid            String        @unique
    form_uid        String
    name            String
    description     String
    input_type_slug String
    choice_list     String?
    created_at      DateTime      @default(now())
    updated_at      DateTime?
    deleted_at      DateTime?
    RegisterForm    RegisterForm? @relation(fields: [form_uid], references: [uuid])

    @@map("question_form")
}

model RegisterForm {
    uuid      String         @unique
    questions QuestionForm[]
}
